{
  "name": "wa-group-scrape-clean",
  "version": "3.0.2",
  "description": "WhatsApp Group Scraper for real estate data - Clean version 3.0.2",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dev-ui": "echo 'Starting in development mode with UI focus' && electron . --dev",
    "build": "electron-builder --win --config.win.target=portable",
    "publish": "electron-builder --win --publish always --config.win.target=portable",
    "release": "electron-builder --win --publish always --config.win.target=portable",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": {
    "name": "Real Estate Tools",
    "email": "support@realestate.tools"
  },
  "license": "ISC",
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "electron": "28.2.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.5.0",
    "dotenv": "^16.3.1",
    "electron-log": "^5.3.2",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "googleapis": "^129.0.0",
    "iconv-lite": "^0.6.3",
    "openai": "^4.28.0",
    "pino-pretty": "^13.0.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "windows-1255": "^3.0.4"
  },
  "build": {
    "appId": "com.realestate.whatsapp-group-scraper-clean",
    "productName": "WhatsApp Group Scraper",
    "copyright": "Copyright Â© 2024",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "StonerStyle",
        "repo": "WA_groups_public"
      }
    ]
  }
